; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Framework"
config/description="Agentic workflow framework for Claude Code + Godot"
config/version="0.1.0"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")

[autoload]

MCPBridge="*res://addons/mcp_bridge/mcp_bridge_autoload.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[editor_plugins]

enabled=PackedStringArray("res://addons/mcp_bridge/plugin.cfg")

[rendering]

renderer/rendering_method="forward_plus"
